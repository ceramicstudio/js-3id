{
  "name": "@3id/did-manager",
  "version": "0.1.0-alpha.0",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicstudio/js-3id#readme",
  "keywords": [
    "3id",
    "did",
    "manager"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/js-3id",
    "directory": "packages/did-manager"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "yarn build:clean && yarn build:types && yarn build:js",
    "lint": "eslint src test --fix",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "test:ci": "yarn test --ci --coverage",
    "test:server": "rm -rf ./test/data && ../../node_modules/.bin/ceramic daemon --port 7777 --state-store-directory ./test/data --network inmemory",
    "prepare": "yarn build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@3id/common": "^0.4.0-alpha.0",
    "@3id/did-provider": "^0.1.0-alpha.0",
    "@ceramicnetwork/blockchain-utils-linking": "^2.0.0-alpha.0",
    "@ceramicnetwork/http-client": "^2.0.0-alpha.0",
    "@ceramicnetwork/stream-caip10-link": "^2.0.0-alpha.0",
    "@ethersproject/hdnode": "^5.0.8",
    "@glazed/did-datastore": "^0.3.0-alpha.0",
    "@stablelib/sha256": "^1.0.0",
    "caip": "^1.0.0",
    "cross-fetch": "^3.0.6",
    "dids": "^3.0.0-alpha.0",
    "errors-utils": "^0.2.0",
    "rpc-utils": "^0.6.0",
    "store": "^2.0.12",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "@3id/model": "^0.1.0",
    "@3id/test-utils": "^0.4.0-alpha.0",
    "@ceramicnetwork/common": "^2.0.0-alpha.0",
    "did-jwt": "^5.12.4",
    "ipfs-repo": "^13.0.7",
    "jest-environment-3id": "^0.1.0"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc/jest",
        {
          "root": "../.."
        }
      ]
    }
  }
}
