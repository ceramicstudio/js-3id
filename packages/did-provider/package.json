{
  "name": "@3id/did-provider",
  "version": "0.4.1-rc.1",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicstudio/js-3id#readme",
  "keywords": [
    "ceramic",
    "3id",
    "did",
    "provider"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/js-3id",
    "directory": "packages/did-provider"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.14"
  },
  "sideEffects": false,
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "yarn build:clean && yarn build:types && yarn build:js",
    "lint": "eslint src --fix",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "test:ci": "yarn test --ci --coverage",
    "prepare": "yarn build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@3id/model": "^0.4.1-rc.1",
    "@ceramicnetwork/3id-did-resolver": "^2.0.0-rc.0",
    "@ceramicnetwork/http-client": "^2.0.0-rc.0",
    "@ceramicnetwork/stream-tile": "^2.0.0-rc.0",
    "@ethersproject/hdnode": "^5.0.8",
    "@glazed/did-datastore": "^0.3.0-rc.0",
    "@glazed/tile-loader": "^0.2.0-rc.0",
    "@stablelib/random": "^1.0.0",
    "@stablelib/x25519": "^1.0.0",
    "cids": "^1.1.6",
    "dag-jose-utils": "^2.0.0-rc.0",
    "did-jwt": "^6.0.0",
    "did-resolver": "^3.1.5",
    "dids": "^3.0.0-rc.0",
    "fast-json-stable-stringify": "^2.1.0",
    "key-did-provider-ed25519": "^2.0.0-rc.0",
    "key-did-resolver": "^2.0.0-rc.0",
    "rpc-utils": "^0.6.1",
    "store": "^2.0.12",
    "uint8arrays": "^3.0.0"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc/jest",
        {
          "root": "../.."
        }
      ]
    }
  }
}
