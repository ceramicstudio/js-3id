{
  "name": "@3id/model",
  "version": "0.1.0",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicstudio/js-3id#readme",
  "keywords": [
    "ceramic",
    "3id",
    "datamodel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/js-3id",
    "directory": "packages/model"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build:model": "node ./scripts/deploy-model.mjs",
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "yarn build:clean && yarn build:types && yarn build:js",
    "lint": "eslint src test --fix",
    "prepare": "yarn build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@datamodels/3id-keychain": "^0.1.2",
    "@datamodels/identity-accounts-crypto": "^0.1.2",
    "@datamodels/identity-accounts-web": "^0.1.2",
    "@datamodels/identity-profile-basic": "^0.1.2",
    "@glazed/types": "^0.2.0-alpha.0"
  },
  "devDependencies": {
    "@3id/model-manager": "^0.1.0",
    "@ceramicnetwork/http-client": "^2.0.0-alpha.0"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc/jest",
        {
          "root": "../.."
        }
      ]
    }
  }
}
