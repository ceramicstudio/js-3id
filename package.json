{
  "name": "3id-connect",
  "version": "1.0.0-alpha.15",
  "description": "Account management for 3ID",
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "lint": "eslint src",
    "test": "run-p test:build:** && jest",
    "test:ceramic": "ceramic daemon --port 7777",
    "test:build:app": "webpack --config ./test/env/webpack.config.js --mode=development",
    "test:build:iframe": "CERAMIC_API=http://localhost:7777 webpack --config webpack.config.js --mode=development --output-path ./test/env/build --output-filename iframe.js",
    "build:prod:es5": "rm -rf ./lib && babel src --out-dir lib --extensions .ts",
    "build:prod:types": "rm -rf ./types && tsc --emitDeclarationOnly --skipLibCheck",
    "build:prod:dist": "webpack --config webpack.config.js --mode=production --output-filename index.js",
    "build": "run-p build:prod:**",
    "prepublishOnly": "npm run build",
    "dev:iframe:build": "webpack --config webpack.config.js --mode=development --watch",
    "dev:iframe:server:start": "http-server -c-1 -p 30001 public",
    "start": "run-p dev:iframe:**",
    "start:testnet": "CERAMIC_API=https://ceramic-clay.3boxlabs.com npm run start",
    "dev:demo:server:start": "http-server -c-1 -p 30000 example",
    "dev:demo:build": "webpack --config ./example/webpack.config.js --mode=development --watch",
    "start:demo": "run-p dev:demo:** start",
    "start:demo:testnet": "CERAMIC_API=https://ceramic-clay.3boxlabs.com run-p dev:demo:** start:testnet"
  },
  "jest": {
    "preset": "jest-playwright-preset",
    "testPathIgnorePatterns": [
      "node_modules",
      "lib"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3box/3id-connect.git"
  },
  "author": "3box <team@3box.io>",
  "license": "(Apache-2.0 OR MIT)",
  "bugs": {
    "url": "https://github.com/3box/3id-connect/issues"
  },
  "homepage": "https://github.com/3box/3id-connect#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@ceramicnetwork/common": "^0.15.7",
    "@ceramicnetwork/http-client": "^0.9.4",
    "@ceramicstudio/idx": "^0.7.0",
    "@ethersproject/hdnode": "^5.0.8",
    "3id-blockchain-utils": "^1.3.1",
    "3id-did-provider": "^0.5.0",
    "caip": "^0.9.2",
    "dag-jose": "^0.3.0",
    "dids": "^1.1.0",
    "errors-utils": "^0.1.0",
    "multicodec": "^2.0.0",
    "postmsg-rpc": "^2.4.0",
    "rpc-utils": "^0.2.0",
    "store": "^2.0.12",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@ceramicnetwork/3id-did-resolver": "^0.6.8",
    "@ceramicnetwork/cli": "^0.20.6",
    "@ceramicstudio/idx-tools": "^0.7.0",
    "@ethersproject/wallet": "^5.0.10",
    "@portis/web3": "^2.0.0-beta.57",
    "@types/store": "^2.0.2",
    "@types/url-parse": "^1.4.3",
    "@walletconnect/web3-provider": "^1.3.3",
    "authereum": "0.0.4-beta.186",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^2.1.1",
    "css-to-string-loader": "^0.1.3",
    "eslint": "^7.17.0",
    "eslint-config-3box": "^0.2.0",
    "eslint-plugin-jest-playwright": "^0.2.1",
    "fortmatic": "^2.2.1",
    "http-server": "^0.12.3",
    "ipfs": "^0.52.3",
    "jest": "^26.6.3",
    "jest-playwright-preset": "^1.4.3",
    "node-sass": "^4.13.1",
    "npm": "^6.14.11",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.7.1",
    "prettier": "^2.2.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.2",
    "typescript": "^4.1.3",
    "uint8arrays": "^1.1.0",
    "url-loader": "^4.0.0",
    "web3modal": "^1.9.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.1.2"
  }
}
